    "workbench.colorTheme": "Ayu Mirage Bordered",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
          {
            "scope": ["keyword.checkbox-todo"],
            "settings": {
              "foreground": "#FF0000",
              "fontStyle": "bold"
            }
          },
          {
            "scope": ["keyword.event"],
            "settings": {
              "foreground": "#6E3500",
              "fontStyle": "bold"
            }
          },
          {
            "scope": ["keyword.checkbox-malformed"],
            "settings": {
              "foreground": "#FF00FF",
              "fontStyle": "bold"
            }
          },
          {
            "scope": ["keyword.checkbox-right_now"],
            "settings": {
              "foreground": "#0000FF",
              "fontStyle": "bold"
            }
          },
          {
            "scope": ["keyword.checkbox-next"],
            "settings": {
              "foreground": "#0000FF",
              "fontStyle": "bold"
            }
          },
          {
            "scope": ["keyword.checkbox-migrated"],
            "settings": {
              "foreground": "#9370D8",
              "fontStyle": "bold"
            }
          },
          {
            "scope": ["keyword.checkbox-waiting"],
            "settings": {
              "foreground": "#FFA500",
              "fontStyle": "bold"
            }
          },
          {
            "scope": ["keyword.checkbox-done"],
            "settings": {
              "foreground": "#90EE90",
              "fontStyle": "bold"
            }
          },
          {
            "scope": ["keyword.checkbox-strikethrough"],
            "settings": {
              "foreground": "#82d882",
              "fontStyle": "bold"
            }
          },
          {
            "scope": ["keyword.strikethrough-text"],
            "settings": {
              "fontStyle": "italic",
              "foreground": "#00000070",
            }
          },
          {
            "scope": ["keyword.bullet"],
            "settings": {
              "fontStyle": "bold"
            }
          }
        ]
    },
    "todo-tree.highlights.defaultHighlight": {
      "icon": "alert",
      "type": "tag",
      "foreground": "#202020",
      "background": "#CCFFFF",
      "opacity": 60,
      "iconColour": "#0000FF"
    },
    "todo-tree.highlights.customHighlight": {
      "TODO": {
        "foreground": "#006622fd",
        "gutterIcon": true,
        "type": "text",
        "icon": "$(chrome-maximize)",
        "iconColour": "#ff0055"
      },
      "FIXME": {
        "foreground": "#660033",
        "iconColour": "#FFFF66",
        //"icon": "break"
        "gutterIcon": true
      },
      "DONE": {
        "foreground": "#000f66ec",
        "iconColour": "#FFFF66",
        "gutterIcon": true,
        "icon": "check",
        "type": "tag"
      },
      "BUG": {
        "foreground": "#AA0000",
        "iconColour": "#FFFF66",
        "gutterIcon": true
      },
      "BLOCKED": {
        "foreground": "#aa0063",
        "iconColour": "#FFFF66",
        "gutterIcon": true,
        "icon": "$(bell-slash)"
      }
    },
    "todo-tree.general.tags": [
      "BUG",
      "HACK",
      "FIXME",
      "TODO",
      //"XXX",
      "[ ]",
      "[x]",
      "DONE",
      "BLOCKED"
    ],
    "git.enableSmartCommit": true,
    "workbench.iconTheme": "vscode-icons",
    "editor.linkedEditing": true,
    "workbench.productIconTheme": "fluent-icons",
    "indentOneSpace.workOnSingleLine": true, // Even if 1 character on 1 line is selected - the command will work.
    "indentOneSpace.cramReversed": true, // Outdent lines even if one of them has reached column 0 (gutter).
    "indentOneSpace.onlyCompleteRange": false,
    "tabnine.experimentalAutoImports": true, // Indent works only when selection has nothing or whitespace characters on the sides
}